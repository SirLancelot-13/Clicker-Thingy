generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          Int     @id @default(autoincrement())
  googleId    String  @unique
  username    String  @unique
  displayName String?
  clicks      Click?

  @@map("users")
}

model Click {
  id       Int    @id @default(autoincrement())
  username String @unique
  count    Int    @default(0)
  user     User   @relation(fields: [username], references: [username])
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}
